{
    "version": 2,
    "builds": [
        {
            "src": "apps/api/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "apps/web/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/(.*)",
            "dest": "apps/api/main.py"
        },
        {
            "src": "/api/docs",
            "dest": "apps/api/main.py"
        },
        {
            "src": "/api/openapi.json",
            "dest": "apps/api/main.py"
        },
        {
            "src": "/category/(.*)",
            "dest": "apps/web/category/[slug]?slug=$1"
        },
        {
            "src": "/food/(.*)",
            "dest": "apps/web/food/[slug]?slug=$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "apps/web/$1"
        }
    ]
}